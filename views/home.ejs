<%- include('partials/header'); -%>
<h1>Home</h1>
<p><%= startingContent %></p>

<% posts.forEach(function(post){ %>
<h1><%= post.text1 %></h1>

<p>
  <%= post.postBody.substring(0, 100) + ' .....' %>
  <a class="btn btn-primary" href="/posts/<%= post.text1 %>">Read More</a>
</p>
<% }) %> <%- include('partials/footer'); -%>
